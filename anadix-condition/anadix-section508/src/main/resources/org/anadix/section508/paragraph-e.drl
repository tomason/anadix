# Redundant text links shall be provided for each active region of
#a server-side image map. 
package org.anadix.section508

import org.anadix.html.*
import org.anadix.section508.report.*

rule "server side map present"
	when
		$element : ImgTag( ismap != null )
	then
		insert(new ImgIsMap($element));
end
